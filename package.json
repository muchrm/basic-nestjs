    "body-parser": "^1.18.2",
    "class-validator": "^0.7.2",
    "jsonwebtoken": "^8.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
